let result=$1+$2
if [ $# -eq 2 ] 
then
if [ $1 -gt $2 ] 
then
echo "$1 é maior que $2. "
 echo "A soma dos números é: $result"
else
if [ $1 -eq $2 ] 
then
echo "Os números são iguais. "
echo "A soma dos números é: $result"
else
echo "$2 é maior que $1. "
echo "A soma dos números é: $result"
fi
fi
else
 echo "Informe dois números"
fi
